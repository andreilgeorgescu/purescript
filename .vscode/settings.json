{
  "files.readonlyInclude": {
    "**/node_modules/**": true,
    "**/.spago/**": true,
    "**/.psci_modules/**": true,
  },
  "explorer.fileNesting.patterns": {
    "spago.yaml": "spago.lock, .purs-repl"
  },
  "purescript.addNpmPath": true,
  "files.watcherExclude": {
    "**/.spago/**": true,
    "**/output/**": true,
    "**/.psci_modules/**": true,
 },
 "search.exclude": {
  "**/node_modules": true,
  "**/bower_components": true,
  "**/*.code-search": true,
  "**/.spago/**": true,
  "**/output/**": true,
  "**/.psci_modules/**": true,
 },
  "editor.defaultFormatter": "nwolverson.ide-purescript",
  "purescript.exportsCodeLens": false,
  "purescript.censorWarnings": [
    "WildcardInferredType"
  ],
}
